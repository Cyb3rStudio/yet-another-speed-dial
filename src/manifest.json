{
  "manifest_version": 2,
  "name": "yet another speed dial",
  "description": "a modern speed dial for firefox and chrome",
  "version": "1.3.16",
  "author": "dev@conceptualspace.net",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "bookmarks",
    "contextMenus",
    "storage",
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "yet_another_speed_dial@conceptualspace.net",
      "strict_min_version": "55.0"
    }
  }
}
